{
  "name": "loft-system",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "heroku:start": "node bin/init && node bin/www",
    "heroku:dev_tmpl": "NODE_ENV=production COOKIE_TIMEOUT=10 MONGODB_URI=... USER_IMAGE={} node bin/init && node bin/www",
    "start": "NODE_ENV=production node bin/www",
    "start_win": "set NODE_ENV=production&&node bin/www",
    "dev": "DEBUG=server:*,socket ./node_modules/.bin/nodemon bin/www",
    "dev_win": "set DEBUG=server:serv,server:api,socket&&set NODE_ENV=development&&nodemon bin/www",
    "initapp": "node ./bin/init"
  },
  "dependencies": {
    "debug": "^2.6.3",
    "gm": "^1.23.1",
    "koa": "^2.2.0",
    "koa-body": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^2.0.1",
    "koa-mongoose": "^2.0.0",
    "koa-multer": "^1.0.2",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "mongoose": "^4.13.7",
    "pug": "^2.0.0-rc.1",
    "socket.io": "^2.0.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.8.1"
  },
  "engines": {
    "node": "9.2.x",
    "npm": "5.6.x"
  }
}
